<Window x:Class="Talent.CarMeasureConfig.SystemConfig.SystemConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="系统配置" Height="500" Width="800" Loaded="Window_Loaded">
    <Grid HorizontalAlignment="Left" Height="469" VerticalAlignment="Top" Width="792">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="139*"/>
            <ColumnDefinition Width="156*"/>
            <ColumnDefinition Width="497*"/>
        </Grid.ColumnDefinitions>
        <TreeView HorizontalAlignment="Left" x:Name="configFiles" Height="469" VerticalAlignment="Top" Width="139" SelectedItemChanged="configFiles_SelectedItemChanged"/>
        <TreeView Grid.Column="1" HorizontalAlignment="Left" x:Name="configNodeNames" Height="469" VerticalAlignment="Top" Width="156" SelectedItemChanged="configNodeNames_SelectedItemChanged">
        </TreeView>
        <Grid Grid.Column="2" x:Name="MainGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="90*"/>
                <ColumnDefinition Width="156*"/>
            </Grid.ColumnDefinitions>
        </Grid>
    </Grid>
</Window>
