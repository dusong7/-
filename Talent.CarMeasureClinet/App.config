    <add key="getPrintModelData" value="http://192.168.100.12:8080/MeasureService/PrintController/rtPrintModelData.do?jsonParams={0}"/>
     <add key="PrintPostModelData" value="http://192.168.100.12:8080/MeasureService/PrintController/PrintPostModelData.do"/>
    <!--获取称点的配置信息-->
    <add key="getEquParamInfo" value="http://192.168.100.12:8080/MeasureService/MeasureSeatController/getEquParamInfo.do?jsonParams={0}" />
    <!--获取称点的配置信息-->
    <add key="getconfigbyip" value="http://192.168.100.12:8080/MeasureService/equipment/getconfigbyip.do?ip={0}" />
