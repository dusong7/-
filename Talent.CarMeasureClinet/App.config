    <add key="ClientCodeInfo" value="/configlist/module[@name='系统设置']/submodule[@name='系统设置']/param[@name='ClientCode']/@value"/>
    <!--终端名称-->
    <add key="ClientNameInfo" value="/configlist/module[@name='系统设置']/submodule[@name='系统设置']/param[@name='ClientName']/@value"/>
    <!--终端Ip-->
    <add key="ClientIpInfo" value="/configlist/module[@name='系统设置']/submodule[@name='系统设置']/param[@name='ClientIp']/@value"/>
