    <add key="ConfirmTime" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='ConfirmTime']/@value"/>
    <!--自动启动时间-->
    <add key="AutoRunTime" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='AutoRunTime']/@value"/>
    
    <!--是否确认重量-->
    <add key="ConfirmWeight" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='IsConfirmWeight']/@value"/>
    <!--是否保存所有重量曲线-->
    <add key="IsSaveAllWeightData" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='IsSaveAllWeightData']/@value"/>
    <!--是否标准键盘-->
    <add key="IsStandardBoard" value="/configlist/module[@name='外设设置']/submodule[@name='键盘设置']/param[@name='IsStandardBoard']/@value"/>
