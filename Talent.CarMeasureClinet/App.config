    <add key="SaveCheatMsg" value="http://192.168.2.42:6080/Logistical/unauth/cheatlog/insert?msg={0}$equipmentname={1}$reserve1={2}$reserve2={3}"/>
    <!--系统更新配置-->
    <add key="FtpAddress" value="192.168.1.106/Update/Client"/>
    <add key="FtpUserName" value="photo"/>
    <add key="FtpPassWord" value="talent"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
    <!--1表示界面显示测试内容，0为不显示-->
    <add key="IsTest" value="1"/>

  
    <!--重发任务时间(S)-->
    <add key="ResendTaskTimer" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='ResendTaskTimer']/@value"/>
