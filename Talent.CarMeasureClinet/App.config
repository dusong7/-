    <add key="getOracleDateTime" value="http://192.168.100.12:8080/MeasureService/tools/getOracleDateTime.do" />
    <!--根据衡器编码获取最大量程 2016-5-20 14:50:41 get-->
    <add key="queryrange" value="http://192.168.100.12:8080/MeasureService/measure/queryrange.do?equcode={0}" />
    <!--调拨业务用户选择业务号之后的服务地址-->
    <add key="inputPlanidInfo" value="http://192.168.100.12:8080/MeasureService/measure/inputPlanidInfo.do"/>
