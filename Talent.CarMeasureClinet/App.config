    <add key="StartupInfo" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='StarType']/@value"/>
    <!--计量方式-->
    <add key="MeasureTypeInfo" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='MeasureType']/@value"/>
