    <add key="MeasureTimePeriodInfo" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='MeasureTimePeriod']/@value"/>
    <!--取数次数(S)-->
    <add key="WeightTimeCountInfo" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='WeightTimeCount']/@value"/>
    <!--允差-->
    <add key="WeightAllowanceInfo" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='WeightAllowance']/@value"/>
    <!--计量单位-->
    <add key="WeightUnitInfo" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='WeightUnit']/@value"/>
