    <add key="IsUserNotice" value="/configlist/module[@name='显示屏设置']/submodule[@name='汽车衡终端']/param[@name='IsUserNotice']/@value"/>
    <!--客户端提示服务地址-->
    <add key="getCardInfo" value="http://192.168.100.12:8080/MeasureService/card/getCardInfo.do?cardid={0}$recordtype={1}$icflag={2}$weightno={3}"/>
    <add key="getMeasureInfo" value="http://192.168.100.12:8080/MeasureService/measure/getMeasureInfobyCarno.do?carno={0}$icid={1}$rfid={2}$caller={3}$planid={4}$weightno={5}$carflag={6}"/>
    <add key="beforesaveMeasureInfo" value="http://192.168.100.12:8080/MeasureService/measure/beforesaveMeasureInfo.do" />
    <add key="saveMeasureInfo" value="http://192.168.100.12:8080/MeasureService/measure/saveMeasureInfo.do"/>
