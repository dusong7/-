<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--Xpath配置-->
    <add key="SysConfigFileName" value="ClientConfig\SystemConfig.xml"/>
    <!--乙方公司-->
    <add key="SysInfo" value="/configlist/module[@name='系统设置']/submodule[@name='系统设置']/param[@name='UnitName']/@value"/>
