    <add key="ZZStr" value="/configlist/module[@name='显示屏设置']/submodule[@name='汽车衡终端']/param[@name='ZZStr']/@value"/>
    <!--系统提示文字-->
    <add key="XTTSStr" value="/configlist/module[@name='显示屏设置']/submodule[@name='汽车衡终端']/param[@name='XTTSStr']/@value"/>
    <!--系统异常文字-->
    <add key="XTYCStr" value="/configlist/module[@name='显示屏设置']/submodule[@name='汽车衡终端']/param[@name='XTYCStr']/@value"/>
