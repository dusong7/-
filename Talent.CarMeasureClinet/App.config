    <add key="saveWeightRecordInfo" value="http://192.168.100.12:8080/MeasureService/equipment/weighterdata.do"/>
    <!--票据打印后回调服务的服务地址-->
    <add key="savePrintBill" value="http://192.168.100.12:8080/MeasureService/MeasureTaskReport/savePrintBill.do?jsonParams={0}"/>
