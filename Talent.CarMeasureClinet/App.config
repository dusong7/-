    <add key="SoftClearSendTime" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='SendTime']/@value"/>
   
    
    <!--是否软件清零-->
    <add key="IsSoftClear" value="/configlist/module[@name='业务设置']/submodule[@name='计量设置']/param[@name='IsSoftClear']/@value"/>
